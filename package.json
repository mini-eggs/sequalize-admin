{
  "name": "@minieggs40/sequalize-admin",
  "version": "0.0.1",
  "description": "An admin interface for Sequalize. Built with Vue, Redux, and friends.",
  "license": "MIT",
  "repository": "https://github.com/mini-eggs/Sequalize-Admin",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run dev-client & npm run dev-server",
    "dev-client": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config webpack.config.client.js",
    "dev-server": "cross-env NODE_ENV=development webpack --watch --hot --config webpack.config.server.js & nodemon dist/server.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vue2-redux": "^1.0.1"
  }
}
